  if PlanSalesUnit.Value <> 0
    then begin
    ActivityPlanSales.value := PlanSalesUnit.Value * PlanBilledUnits.Value;
    if fGPPC <> 0 then
      ActivityPlanGP.value := ActivityPlanSales.Value * fGPPC / 100;
    CalculatePlanActivity(SALES);
  end;
end;

