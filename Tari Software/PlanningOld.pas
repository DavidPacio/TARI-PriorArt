procedure TPlanningOldF.PlanWizardPage1Enter(Sender: TObject);
var
  j: Integer;
//  cindex : String;
begin
  Initialised := False;
  tabpress := false;
  PlanExtBtn.Enabled := False;
  Caption := 'Targeting - ' + DataModuleClient.BusinessDetailsBusiness_Name.value;
  Label1.caption := 'Establishing Dates';
  for j := 0 to DETAILPLANGRAPHLIST - 1 do    // Iterate
  begin
    ChartSeries[j] := nil;
  end;    // for
//  Cindex := DataModuleClient.wwDateRef.IndexName;
//  DataModuleClient.wwDateRef.IndexName := 'ASCDates';
  DataModuleClient.DateRef.First;
  J := 0;
  while DataModuleClient.DateRef.eof = False do
    begin
    inc(J);
    DataModuleClient.DateRef.next;
  end;    // while

  if J < 5 then
    wwDBNavigator1Insert.Visible := True
  else
    wwDBNavigator1Insert.Visible := False;
  InitialPlanStart := DataModuleClient.DepartmentsCurrentPlanStart.Value;
  InitialPlanEnd := DataModuleClient.DepartmentsCurrentPlanEnd.Value;
end;

procedure TPlanningOldF.PlanningWizardFinishClick(Sender: TObject);
begin
//  OvcTable3.Invalidate;
  screen.Cursor := crHourGlass;
  if (OvcTable3.ActiveRow > 2) and (OvcTable3.ActiveRow <= OvcTable3.Rowlimit) then
    OvcTable3.SetActiveCell(OvcTable3.ActiveRow - 1, OvcTable3.ActiveCol)
  else
    OvcTable3.SetActiveCell(OvcTable3.ActiveRow + 1, OvcTable3.ActiveCol);
  OvcTable3.StopEditingState(true);
  OvcTable3.StartEditingState;
  Ovctable1.StopEditingState(true);
  Ovctable1.StartEditingState;
