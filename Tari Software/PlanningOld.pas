        begin
        DataType := 1;
        VCount := OvcTable3.RowLimit - 1;
        Cumulative := False;
        DateTime := True;
        VDArray := @ChartDates;
}
//      end;    // with
//      ReplaceList(YValues, TValueList.Create(ChartSeries[index], 'Y'));
//      ChartSeries[Index].YValues.Count := OvcTable3.RowLimit - 1;
{      with TValueList(YValues) do
