procedure TPlanningOldF.BuilddateArray;
var
  j: Integer;
begin
  Fillchar(ChartDates, sizeof(TDateTimeArray), #0);
  for j := 0 to OvcTable3.rowlimit - 2 do    // Iterate
    begin
    if ComboBox1State = 0 then
      ChartDates[j] := DataModuleClient.DepartmentsCurrentPlanStart.value + j
    else
      ChartDates[j] := DataModuleClient.DepartmentsCurrentPlanStart.value + SummaryPeriodIndexArray[j];
  end;    //
  Chart1.BottomAxis.Maximum := ChartDates[OvcTable3.RowLimit - 2] + 1;
  Chart1.BottomAxis.Minimum := ChartDates[0] - 1;
//  Chart1.BottomAxis.DateTimeFormat := 'dd' + #13 + 'MMM';
//  Chart1.BottomAxis.
  //abelStyle := talMark;
end;

procedure TPlanningOldF.DeleteSeries(index : Integer);
begin
  if ChartSeries[Index] <> nil
    then begin
    chart1.RemoveSeries(ChartSeries[Index]);
    ChartSeries[Index].Clear;
    Chartseries[Index].Free;
//    ChartSeries[Index].destroy;
    Chartseries[Index] := nil;
  end;    // for
end;

procedure TPlanningOldF.BuildSeries(index: integer);
var
  I: Integer;
//  k: Integer;
  PlanDataRec : TPlanDataRecPtr;
//  Memptr : Pointer;
  ArraySize : Integer;
  YValue, YAccum : Currency;
  TempStr : String;
begin
  PlanDataRec := PlanDataList.Items[GetaccountListIndex(DetailPlanGraph[Index])];
  ArraySize := OvcTable3.RowLimit - 1;
  if (CheckListBox1.Checked[Index]) and (ChartSeries[index] = nil)
    then begin
    ChartSeries[index] := TLineSeries.create(Chart1);
    if (PlanDataRec.Accountno = POTENTIAL) or (PlanDataRec.Accountno = BILLED)
      or (PlanDataRec.Accountno >= 10000) then
      ChartSeries[index].Title := GetUnitTitle(PlanDataRec.AccountNo)
    else
      ChartSeries[index].Title := PlanDataRec.AccountTitle;
    Chartseries[index].XValues.DateTime := True;
    ChartSeries[Index].XValues.Modified := True;
    Chartseries[Index].YValues.Modified := True;
    YAccum := 0;
    for I := 0 to ArraySize - 1 do    // Iterate
      begin
      if ComboBox1State = 0 then
        YValue := PlanDataRec.PlanDetail[I]
      else
        YValue := PlanDataRec.PlanSummary[I];
      if (PlanDataRec.Accountno < 10000) and (CheckBox2.Checked)
        then begin
        YValue := YValue + YAccum;
        YAccum := YValue;
      end;
      Tempstr := FormatDateTime('dd' + #13 + 'MMM', ChartDates[I]);
      ChartSeries[Index].AddXY(ChartDates[I], YValue, tempstr, clTeeColor);
//      ChartSeries[Index].XLabel[I] := tempstr;
    end;    // for
//    ChartSeries[Index].YValues.value := TChartValues(memPtr);
{   ReplaceList(XValues, TValueList.Create(ChartSeries[index], 'X'));
