function TCustomRegIniFile.iniFetchDirectory(const nameS : string; defaultS : string) : string;
begin
  result := iniFetchString(nameS, defaultS);
  convertDirectory(result, FBaseDirInIniS, FBaseDirS);
end;

procedure TCustomRegIniFile.iniWriteDirectory(const nameS : string; valueS : string);
begin
  iniWriteString(nameS, valueS);
end;

procedure TCustomRegIniFile.WriteDirectory(const nameS : string; valueS : string);
begin
  convertDirectory(valueS, FBaseDirS, FBaseDirInIniS);
  WriteString(nameS, valueS);
end;

// reads ini for system value, or defaultS
// and reads reg for local value, or default of ''
// returns ini value as fn return, and reg value via var regValueS
// If floppyB is true allows a:\ or b:\ for local value. Such values rejected in all other cases.
function TCustomRegIniFile.regIniFetchDirectory(const nameS : string; defaultS: string; var regValueS : string; floppyB: boolean) : string;
begin
  result    := iniFetchDirectory(nameS, defaultS);
  if isFloppy(result) = true then
    result  := defaultS;
  regValueS := regFetchDirectory(nameS);
  if (floppyB = false) and (isFloppy(regValueS) = true) then
    regValueS := '';
end;

procedure TCustomRegIniFile.regIniWriteDirectory(const nameS : string; regValueS, iniValueS : string);
begin
  convertDirectory(iniValueS, FBaseDirS, FBaseDirInIniS);
  regIniWriteString(nameS, regValueS, iniValueS);
end;

