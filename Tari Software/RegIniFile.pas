// 10.07.99 djh Temporary AW reabability check removed. Base Directory check added to TRegAndIniFile constructor
constructor THiIniFile.Create(const fileName : string);
begin
  inherited Create(FileName);
  FSections := TStringList.Create;
  LoadValues(false); // false = don't hold file open exclsuively i.e. read and close the file
end;

destructor THiIniFile.Destroy;
begin
  updateFile;  // writes ini file if it has been written to and updateFile hasn't been called
  if FSections <> nil then
    Clear;
  FSections.Free;
end;

procedure THiIniFile.Clear;
var
  I: Integer;
begin
  for I := 0 to FSections.Count - 1 do
    TStrings(FSections.Objects[I]).Free;
  FSections.Clear;
end;

function THiIniFile.AddSection(const Section: string): TStrings;
begin
  Result := TStringList.Create;
  try
    FSections.AddObject(Section, Result);
  except
    Result.Free;
  end;
end;

procedure THiIniFile.EraseSection(const Section: string);
var
  I: Integer;
begin
  I := FSections.IndexOf(Section);
  if I >= 0 then
  begin
    TStrings(FSections.Objects[I]).Free;
    FSections.Delete(I);
  end;
end;


procedure THiIniFile.GetStrings(List: TStrings);
var
  I, J: Integer;
  Strings: TStrings;
begin
  List.BeginUpdate;
  try
    for I := 0 to FSections.Count - 1 do
    begin
      List.Add('[' + FSections[I] + ']');
      Strings := TStrings(FSections.Objects[I]);
      for J := 0 to Strings.Count - 1 do List.Add(Strings[J]);
      List.Add('');
    end;
  finally
    List.EndUpdate;
  end;
end;

procedure THiIniFile.LoadValues(holdFileOpenExclusivelyB : boolean);
var
  List  : TStringList;
  i     : integer;
  mode  : word;
begin
  if FSections <> nil then
    Clear;
  if (FileName <> '') and FileExists(FileName) then begin
    // TMemIniFile.Loadvalue's List.LoadFromFile(FileName) statement replaced by this code, which
    // keeps tries ap to 30 times to open the file, and opens it in different modes depending on
    // whether holdFileOpenExclusivelyB is set or not.
    if holdFileOpenExclusivelyB then
      mode := fmOpenReadWrite or fmShareExclusive // open exclusively, for readingd writing
    else
      mode := fmOpenRead or fmShareDenyWrite;     // open for read only, write access denied
    FStream := nil;
    for i := 0 to 29 do begin
      try
        FStream := TFileStream.Create(FileName, mode);
      except
        Sleep(100 * i);
      end;
      if assigned(FStream) then begin
        List := TStringList.Create;
        try
          try
            List.LoadFromStream(FStream);
          finally
            if not holdFileOpenExclusivelyB then begin
              FStream.Free;
              FStream := nil;
            end;
          end;
          SetStrings(List);
        finally
          List.Free;
        end;
        break;  // out of for loop
      end;
    end;
  end;
  FModifiedB := false;
end;

procedure THiIniFile.ReadSection(const Section: string; Strings: TStrings);
var
  I, J: Integer;
  SectionStrings: TStrings;
begin
  Strings.BeginUpdate;
  try
    Strings.Clear;
    I := FSections.IndexOf(Section);
    if I >= 0 then
    begin
      SectionStrings := TStrings(FSections.Objects[I]);
      for J := 0 to SectionStrings.Count - 1 do
        Strings.Add(SectionStrings.Names[J]);
    end;
  finally
    Strings.EndUpdate;
  end;
end;

procedure THiIniFile.ReadSections(Strings: TStrings);
begin
  Strings.Assign(FSections);
end;

procedure THiIniFile.ReadSectionValues(const Section: string; Strings: TStrings);
var
  I: Integer;
begin
  Strings.BeginUpdate;
  try
    Strings.Clear;
    I := FSections.IndexOf(Section);
    if I >= 0 then Strings.Assign(TStrings(FSections.Objects[I]));
  finally
    Strings.EndUpdate;
  end;
end;

function THiIniFile.ReadString(const Section, Ident, DefaultS: string): string;
var
  I: Integer;
  Strings: TStrings;
begin
  I := FSections.IndexOf(Section);
  if I >= 0 then begin
    Strings := TStrings(FSections.Objects[I]);
    I := Strings.IndexOfName(Ident);
    if I >= 0 then begin
      Result := Copy(Strings[I], Length(Ident) + 2, Maxint);
      Exit;
    end;
  end;
  Result := DefaultS;
end;

procedure THiIniFile.SetStrings(List: TStrings);
var
  I: Integer;
  S: string;
  Strings: TStrings;
begin
  Clear;
  Strings := nil;
  for I := 0 to List.Count - 1 do begin
    S := List[I];
    if (S <> '') and (S[1] <> ';') then
      if (S[1] = '[') and (S[Length(S)] = ']') then
        Strings := AddSection(Copy(S, 2, Length(S) - 2))
      else
        if Strings <> nil then Strings.Add(S);
  end;
end;

procedure THiIniFile.UpdateFile;
var
  List: TStringList;
begin
  if FModifiedB then begin
    if assigned(FStream) then begin // this should be true if FModifiedB is true
      List := TStringList.Create;
      try
        GetStrings(List);
      //List.SaveToFile(FileName);
        try
          SetFilePointer(FStream.handle, 0, nil, FILE_BEGIN); // |- truncate the file as we are about to write the lot
          SetEndOfFile(FStream.handle);                       // |
          List.SaveToStream(FStream);
        finally
          FStream.Free;
          FStream := nil;
        end;
      finally
        List.Free;
      end;
    end;
    FModifiedB := false;
  end;
end;

procedure THiIniFile.WriteString(const Section, Ident, Value: String);
var
  I: Integer;
  S: string;
  Strings: TStrings;
  firstModifyingWriteB : boolean;
begin
  firstModifyingWriteB := false;
  I := FSections.IndexOf(Section);
  if I >= 0 then
    Strings := TStrings(FSections.Objects[I])
  else
    Strings := AddSection(Section);
  S := Ident + '=' + Value;
  I := Strings.IndexOfName(Ident);
  if I >= 0 then begin
    if S <> Strings[I] then begin
      if not FModifiedB then
        firstModifyingWriteB := true
      else
        Strings[I] := S;
    end;
  end else begin
    if not FModifiedB then
      firstModifyingWriteB := true
    else
      Strings.Add(S);
  end;
  if firstModifyingWriteB then begin
    // first write that is a change and which will result in a subsequent file write, so get the file exclsuively
    LoadValues(true); // true = hold file open exclsuively in preparation for a write
    FModifiedB := true;
    WriteString(Section, Ident, Value);
  end;
end;

