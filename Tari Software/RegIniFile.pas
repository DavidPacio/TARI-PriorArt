begin
  raise EBadBaseDirOrIni.Create('Call to unimplemented THiIniFile.DeleteKey');
end;


{
procedure THiIniFile.Rename(const FileName: string; Reload: Boolean);
begin
  FFileName := FileName;
  if Reload then LoadValues;
end;
