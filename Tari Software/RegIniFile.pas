function TCustomRegIniFile.FetchAsInteger(const nameS : string; defaultI : integer) : integer;
begin
  try
    Result := StrToInt(FetchString(nameS, IntToStr(defaultI)));
  except
    on E: ERegistryException
    do begin
      if Copy(E.Message, 1, 18) = 'Invalid data type ' then
        deleteValue(nameS);
      result := defaultI;
    end;
    else raise;
  end;
end;
