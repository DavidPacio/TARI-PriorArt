constructor TRegAndIniFile.Create(baseDirOverridePS : string);
var
  shortIniFileNameS, fullIniFileNameS : string;
  baseKeyS : string;
begin
  case getProgramCode of
    hAccountant: begin
                   shortIniFileNameS := 'ha.ini';
                   BaseKeyS          := HARTLEY_HA_REGISTRY_KEY;
                 end;
    hBilling:    begin
                   shortIniFileNameS := 'hb.ini';
                   BaseKeyS          := HARTLEY_HB_REGISTRY_KEY;
                 end;
    hConsultant: begin
                   shortIniFileNameS := 'hc.ini';
                   BaseKeyS          := HARTLEY_HC_REGISTRY_KEY;
                 end;
  end;

//inherited Create(baseDirOverridePS, baseKeyS);
  inherited Create(baseKeyS);

  if baseDirOverridePS <> '' then
    FBaseDirS := baseDirOverridePS
  else begin
    FBaseDirS := inherited readString('Base Directory');   // reg
    if FBaseDirS = '' then begin
      FBaseDirS := 'c:\hc';    // 25.02.97 djh Default changed from '\HC'
      writeString('Base Directory', FBaseDirS);            // reg
    end;
  end;

  // grw 11.06.97 don't accept illegal base directories without a fight!
  if not DirectoryExists(FBaseDirS) then
    raise EBadBaseDirOrIni.Create('The specified base folder ''' + FBaseDirS + ''' does not exist. Please check the registry and command line settings.');
  fullIniFileNameS := FBaseDirS + '\' + shortIniFileNameS;
  if not FileExists(fullIniFileNameS) then
    raise EbadBaseDirOrIni.Create('There is no ''' + shortIniFileNameS + ''' file in the specified base folder ''' + FBaseDirS + '''. Please create or copy a suitable file, or select another base folder.');

  IniFile        := THiIniFile.Create(fullIniFileNameS);
  FBaseDirInIniS := iniFetchString('Base Directory', baseDirOverridePS); // fetch the base directory as described by the ini file
  if FBaseDirInIniS = '' then
      raise EbadBaseDirOrIni.Create('The ''' + shortIniFileNameS + ''' file in the specified base folder ''' + FBaseDirS + ''' does not contain a mandatory ''Base Directory'' entry. Please create or copy a suitable file, or select another base folder.');
end;

function TRegAndIniFile.ReadString(const nameS : string) : string;   // reg then ini
begin
  try
    result := inherited readString(nameS);                      // reg
  except
    result := '';
  end;
  if result = '' then
    result := IniFile.readString(FcurrentSection, nameS, '');   // ini
end;

