// if dirS = findS set dirS to replaceS
// if first part of dirS = findS and dirS follows on with '\...' then set dirS to replaceS + the '\...' extra in dirS
// o'wise leave dirS unchanged
procedure convertDirectory(var dirS : string; findS, replaceS : string);
var
  len : integer;
begin
  len := Length(findS);
  if strLIComp(PChar(dirS), PChar(findS), len) = 0 then begin
    // dirS = findS for length of findS, case insensitive
    if length(dirS) = len then
      // dirS = findS
      dirS := replaceS
    else if dirS[len+1] = '\' then
    //dirS := replaceS + Copy(dirS, Length(findS)+1, Length(dirS));
      dirS := replaceS + Copy(dirS, len+1, 999);
  end
end;

