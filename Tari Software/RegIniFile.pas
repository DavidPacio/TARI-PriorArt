function TCustomRegIniFile.fetchFont(const nameS : string; form : TForm; DefaultFont : TFont; Font : TFont;
  scaler, divis : integer) : TFont;
var
  oldSection : string;
  r : integer;
//  scaler, divis : integer;
begin
  oldSection := currentSection;
  currentSection := currentSection + '\' + nameS;

  r := 0;
  with DefaultFont
  do begin
    if fsBold in Style then r := r + 1;
    if fsItalic in Style then r := r + 2;
    if fsUnderline in Style then r := r + 4;
    if fsStrikeout in Style then r := r + 8;
  end;
  r := fetchInteger('Style', r);
  with Font
  do begin
    Style := [];
    if r mod 2 = 1 then
      Style := Style + [fsBold];
    if (r shr 1) mod 2 = 1 then
      Style := Style + [fsItalic];
    if (r shr 2) mod 2 = 1 then
      Style := Style + [fsUnderline];
    if (r shr 3) mod 2 = 1 then
      Style := Style + [fsStrikeout];
  end;

  with DefaultFont
  do begin
    if Pitch = fpFixed then
      r := 1
    else if Pitch = fpVariable then
      r := 2
    else
      r := 0;
  end;
  r := fetchInteger('Pitch', r);
  with Font
  do begin
    case r of
      0 : Pitch := fpDefault;
      1 : Pitch := fpFixed;
      2 : Pitch := fpVariable;
      else
        Pitch := fpDefault;
    end;
  end;

  font.height := fetchInteger('Height', DefaultFont.Height);
  font.name   := fetchString('Name', DefaultFont.Name);
  font.color  := fetchInteger('Color', DefaultFont.Color);

  try
    font.size := readInteger('Point Size'); // scale up the font size from its 640x480 equivalent
//    (form as THiForm).GetScaleFactors(scaler, divis);
    if form.PixelsPerInch <> 96
    then begin
      scaler := scaler * 96;
      divis := divis * form.PixelsPerInch;
    end;
    font.size := MulDiv(font.size, scaler, divis);
  except
    font.size := DefaultFont.Size;
  end;

  currentSection := oldSection;
  result := font;
end;

procedure TCustomRegIniFile.writeFont(const nameS : string; form : TForm; font : TFont;
  scaler, divis : integer);
var
  oldSection : string;
  r : integer;
//  scaler, divis : integer;
begin
  oldSection := currentSection;
  currentSection := currentSection + '\' + nameS;

  writeString('Name', font.Name);
  writeInteger('Color', font.Color);

//  (form as THiForm).GetScaleFactors(scaler, divis);
  if form.PixelsPerInch <> 96
  then begin
    scaler := scaler * 96;
    divis := divis * form.PixelsPerInch;
  end;

  writeInteger('Point Size', MulDiv(font.Size, divis, scaler)); // scale down the font size to its 640x480 equivalent

  writeInteger('Height', font.Height);

  r := 0;
  with Font
  do begin
    if fsBold in Style then r := r + 1;
    if fsItalic in Style then r := r + 2;
    if fsUnderline in Style then r := r + 4;
    if fsStrikeout in Style then r := r + 8;
  end;
  writeInteger('Style', r);

  with Font
  do begin
    if Pitch = fpFixed then
      r := 1
    else if Pitch = fpVariable then
      r := 2
    else
      r := 0;
  end;
  writeInteger('Pitch', r);

  currentSection := oldSection;
end;

