{ -- setCurrentYearKey - Write procedure for the currentYearKey property.      }
{                        Sets the current and last year keys                   }
{ ---------------------------------------------------------------------------- }
procedure THCAccountingEngine.setCurrentYearKey(Value: string);
var
  yearNo: integer;
  yearStr: string;
begin

  FcurYearDateKey := Value;

  // Build last year Key
  try
    yearStr := Copy(FcurYearDateKey, 11, 4);
    yearNo := StrToInt(yearStr);
  except
    yearNo := 0;
  end;
  if yearNo > 0 then
    FlastYearDateKey := Copy(FcurYearDateKey, 1, 10)+Format('%.4d', [yearNo-1])+FcurYearDateKey[15]
  else
    FlastYearDateKey := '999';

end;

{ ---------------------------------------------------------------------------- }
{ -- buildMonthBalanceKey - Build the unique key for the monthly balance files }
{                           The key consists off:                              }
{                                                                              }
{                           123456789012345678901234                           }
{                           DDDDDDDDDDDDDDDAAAAAAAAS                           }
{                                                                              }
{                           DDDDDDDDDDDDDDD = The division date key            }
{                           AAAAAAAA = The account code                        }
{                           S = Section Type                                   }
{ ---------------------------------------------------------------------------- }
function THCAccountingEngine.buildMonthBalanceKey(year: TBalanceYear; accountCode:PChar): string;
begin

  // See which year we want
  if year = byThisYear then
    result := FcurYearDateKey
  // if requested last year then make sure there is one to search by
  else begin
    if FlastYearDateKey = '999'
    then begin
      result := '';
      exit;
    end;
    result := FlastYearDateKey;
  end;

  result := result + strPrePadChar(trimleft(accountCode), '0', 8);
  result := result + sectionFlags[ord(FdataSection)];
end;

