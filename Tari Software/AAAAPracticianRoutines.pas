{ ---------------------------------------------------------------------------- }
{ -- roundBalanceArrayToThousands - The amounts in the balance array will be   }
{                                   converted to whole amount and then divided }
{                                   by a thousand.
{ ---------------------------------------------------------------------------- }
procedure roundBalanceArrayToThousands(var balances1: array of currency);
var
  balIndex: integer;
begin

  for balIndex := low(balances1) to high(balances1) do
    balances1[balIndex] := balances1[balIndex] / 1000;

  // Strip Trailing spaces
  roundBalanceArray(balances1, 0);
end;

