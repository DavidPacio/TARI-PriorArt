// Returns the percentage that Value1 is of Value2
function getPercentageOf(Value1, Value2: currency ): currency;
begin
  if Value2 = 0 then
    Result := 0
  else begin
    try
      result  := (Value1/Value2)*100
    except
      on EZeroDivide do result := 0;
      on EInvalidOp do result := 0;
    end;
  end;
end;

// Return the percentage change between value1 and value 2
function getPercentageChange(Value1, Value2: currency ): currency;
begin
  If Value1 = 0 then
    Result := 0
  else begin
    try
      result  := ((Value2-Value1)*100)/Value1;
    except
      on EZeroDivide do result := 0;
      on EInvalidOp do result := 0;
    end;
  end;
end;

{ ---------------------------------------------------------------------------- }
{ -- Balance array's manipuations routines                                     }
{ ---------------------------------------------------------------------------- }
{ Courtesy of Trevor Watters

  Routine to manipulate an array of currencys so that the total equals that
  passed. Takes into account the current balance of each elment and uses this
  to calculate the new balances as a percentage of the overall total.

  barray: The array of currencys to manipulate
  astart: The first element in the array to use
  aend:   The last element in the array to use
  new:    The new total the astart to aend element must total too

  Roundto:
  This string can be of zero length in which case maximum
  procision is retained, else it should be one of the following:-
  0         for rounding to the nearest whole currency figure;
  0.0       for rounding to the first decimal place or;
  0.00      etc for further precision.

  11/10/1999 tww Jm's implementation of roundto fails to work correctly. We need
                 an extended which multiplies then divides each element of the array
                 to correctly get the right result.
                 0 means no rounding,
                 1 means move the decimal place to the right , round and move to the left, etc.


  If the current sum or the array is zero, it will divide the New value evenly
  and rounded dynamically if roundto is not zero length.
  If the new value is zero, the array will be initialized.
  In all other cases, the sum of the array will equal the new value by
  apportioning the new value based on the relative values of each element.
  ---------------------------------------------------------------------------- }
function updateArray(var barray: array of currency; astart, aend: integer;
                     new: currency; roundto: Extended): boolean;
var
  temp, temp1, temp2, temp3, temp4, temp5: currency;
  cn: integer;
//  temprnd: extended;
// cc disabled variable above as not used.
  arraysize : integer;
begin
  temp1 := new;
  temp := 0;
  for cn := astart to aend do
    temp := temp + barray[cn];

  // tww 25.10.99 calculation the full size of the array
  arraysize := sizeof(barray) div sizeof(barray[0]);

  if new = 0
  then begin
    for cn := 0 to arraysize - 1 do  // tww 13.10.99 Clear the whole array not just
                                 //              from astart to aend
      barray[cn] := 0;
  end
  else if temp = 0
  then begin
    temp3 := 0;
    temp4 := 0;
    temp2 := aend - (astart - 1);
    temp1 := temp1 / temp2;
    for cn := astart to aend
    do begin
      barray[cn] := temp1;
      if roundto > 0 then
        barray[cn] := round(roundto * 10 * barray[cn]) / (roundto * 10)
      else
        barray[cn] := round(barray[cn]);
      temp3 := temp3 + barray[cn];
      temp4 := temp4 + temp1;
      temp5 := temp4 - temp3;
      if temp5 > 1
      then begin
        barray[cn] := barray[cn] + 1;
        temp3 := temp3 + 1;
      end;
      if temp5 < -1
      then begin
        barray[cn] := barray[cn] - 1;
        temp3 := temp3 - 1;
      end;

      // 12.10.99 tww To fix the last element of the array - NEW
      if cn = aend then barray[cn] := barray[cn] + new - temp3;
    end;
  end
  else begin
    temp4 := new;
    for cn := 0 to arraysize - 1
    do begin
      if (cn >= astart) AND (cn <= aend)
      then begin
        temp3 := barray[cn];
        if (temp4 <> 0) and (temp <> 0) then
          barray[cn] := barray[cn] * temp4 / temp;
        if roundto > 0
        then
          barray[cn] := round(roundto * 10 * barray[cn]) / (roundto * 10)
        else
          barray[cn] := round(barray[cn]);
        temp4 := temp4 - barray[cn];
        temp := temp - temp3;

        if cn = aend  // tww 13.10.99 Not implemented by jm previously to correct the last amount
        then begin
          if roundto > 0 then
            temp4 := round(roundto * 10 * temp4) / (roundto * 10)
          else
            temp4 := round(temp4);
          barray[cn] := barray[cn] + temp4;
        end;
      end
      else
        barray[cn] := 0;
    end;
  end;
  result := True;
end;

{ ---------------------------------------------------------------------------- }
{ -- totalBalanceArray - Returns the total of all values in the passed array   }
{ ---------------------------------------------------------------------------- }
function totalBalanceArray(const balances: array of currency): currency;
var
  balIndex: integer;
begin
  result := 0;

  for balIndex := low(balances) to high(balances) do
    result  := result  + balances[balIndex];
end;

{ ---------------------------------------------------------------------------- }
{ -- totalBalanceArrayRange - Returns the total of values in the array from    }
{                             element rangeFrom to element rangeTo             }
{ ---------------------------------------------------------------------------- }
function totalBalanceArrayRange(const balances: array of currency; rangeFrom, rangeTo: integer): currency;
var
  balIndex: integer;
begin
  result := 0;

  for balIndex := rangeFrom to rangeTo do
    result  := result  + balances[balIndex];
end;
{ ---------------------------------------------------------------------------- }
{ -- subtractBalanceArray - Subtracts the values of elements in Balance2 array }
{                           from corresponding elements in Balance 1 array     }
{ ---------------------------------------------------------------------------- }
procedure subtractBalanceArray(var balances1: array of currency; const balances2: array of currency);
var
  balIndex: integer;
begin
  for balIndex := low(balances1) to high(balances1) do
    balances1[balIndex] := balances1[balIndex] - balances2[balIndex];
end;

{ ---------------------------------------------------------------------------- }
{ -- addBalanceArray - Adds the values of elements in Balance2 array to        }
{                      corresponding elements in Balance 1 array               }
{ ---------------------------------------------------------------------------- }
procedure addBalanceArray(var balances1: array of currency; const balances2: array of currency);
var
  balIndex: integer;
begin
  for balIndex := low(balances1) to high(balances1) do
    balances1[balIndex] := balances1[balIndex] + balances2[balIndex];
end;

{ ---------------------------------------------------------------------------- }
{ -- convertWeeklyBalToMonthly - Converts the weekly balances array to a       }
{                                monthly balance array using the passed month  }
{                                end indicators                                }
{ ---------------------------------------------------------------------------- }
procedure convertWeeklyBalToMonthly(const wBalances: TWeeklyBalArray; var mBalances: TMonthlyBalArray; monthEnds: TMonthEnds);
begin
  // Clear out the monthly balance array
  fillChar(mBalances, sizeof(TMonthlyBalArray), #0);
  addWeeklyBalToMonthly(wBalances, mBalances, monthEnds);
end;

{ ---------------------------------------------------------------------------- }
{ -- addtWeeklyBalToMonthly - Adds the weekly balances array to a monthly      }
{                             balance array using the passed month end         }
{                             indicators                                       }
{ ---------------------------------------------------------------------------- }
procedure addWeeklyBalToMonthly(const wBalances: TWeeklyBalArray; var mBalances: TMonthlyBalArray; monthEnds: TMonthEnds);
var
  curMonthEnd, month: smallint;
  weekNo, mWeekStart: smallint;
begin

  curMonthEnd := 0;
  mWeekStart := 1; // So as to Skip over the brought forward (element zero)
  month := 1;      // Ditto
  // While there is a month end to process
  while monthEnds[curMonthEnd] > -1
  do begin
    // Gor through the weekly balances from the first week of the current month to the last
    for weekNo := mWeekStart to monthEnds[curMonthEnd]+1 do
      mBalances[month] := mBalances[month] +  wBalances[weekNo];

    mWeekStart := monthEnds[curMonthEnd]+2;
    inc(curMonthEnd);
    inc(month);

  end;
end;

{ ---------------------------------------------------------------------------- }
{ -- roundBalanceArray - Rounds each monthly balance to the required Decimal   }
{                        places.                                               }
{ ---------------------------------------------------------------------------- }
procedure roundBalanceArray(var balances1: array of currency; const noDP: smallint);
var
  balIndex: integer;
begin

  for balIndex := low(balances1) to high(balances1) do
    roundCurrency(balances1[balIndex], nodp);
end;

{ ---------------------------------------------------------------------------- }
{ -- roundCurrency - Rounds the passed currency to the required decimal places }
{ ---------------------------------------------------------------------------- }
function roundCurrency(var value: currency; const noDP: smallint): currency;
  function decimalpicture(noDP: smallint):string;
  var
     c : byte;
  begin
    result:='';
    for c := 1 to noDp do
      result := result+'0';
    if noDp > 0 then result := '.'+result;
    result := '0' + result;
    for c := 2 to 3 do   {Could substitute number in thousands group WININI here}
      result := '#'+ result;
    result:= '#'+ result;
  end;
var
  numStr: string;
begin
  // Convert to string rounded to noDP
  numStr := FormatFloat(DecimalPicture(noDp), value);

  // then convert it back
  value := StrToFloat(numStr);
  result := Value;
end;

{ ---------------------------------------------------------------------------- }
{ -- roundResult - Returns the result of the passed value rounded to the       }
{                  required decimal places. Used manily for rounding the       }
{                  results of calculations.                                    }
{ ---------------------------------------------------------------------------- }
function roundResult(value: currency; const noDP: smallint): currency;
begin
  result := roundCurrency(value, noDP);
end;

