{ -- mergeMonthlyBalances - Merges balances2 amounts with balances1, starting  }
{                           with the elemenet 'MonthToStartMerge' until the end}
{                           of the array.                                      }
{ ---------------------------------------------------------------------------- }
procedure mergeMonthlyBalances(var balances1: array of currency; const balances2: array of currency; MonthToStartMerge: integer);
var
  balIndex: integer;
begin

  for balIndex := MonthToStartMerge to high(balances2) do
    balances1[balIndex] := balances2[balIndex];
end;

{ ---------------------------------------------------------------------------- }
{ -- AdjustOpenBalances - Ripple and difference in opening balances through all}
{                         periods of the Actual Balances up to MonthtoUpdate   }
{ ---------------------------------------------------------------------------- }
procedure AdjustOpenBalance(Const Planbalances: array of currency; Var Actualbalances: array of currency; MonthToUpdate: integer);
var
  balIndex: integer;
  diff : currency;
begin
  if PlanBalances[0] <> ActualBalances[0]
  then begin
    diff := Actualbalances[0] - PlanBalances[0];
    for balIndex := 0 to MonthToUpdate do
      Actualbalances[balIndex] := Actualbalances[balIndex] - diff;
  end;
end;


{ ---------------------------------------------------------------------------- }
{ -- setMonthlyBalancesValue - Set all elements of the passed array to value   }
{ ---------------------------------------------------------------------------- }
procedure setMonthlyBalancesValue(var balances: array of currency; value: currency);
var
  balIndex: integer;
begin

  for balIndex := low(balances) to high(balances) do
    balances[balIndex] := value;

end;

{ ---------------------------------------------------------------------------- }
{ -- setMonthlyBalancesValueRange - Set the selected range of elements to the  }
{                                   value passed.                              }
{ ---------------------------------------------------------------------------- }
procedure setMonthlyBalancesValueRange(var balances: array of currency; value: currency; rangeFrom, rangeTo: integer);
var
  balIndex: integer;
begin

  for balIndex := rangeFrom to rangeTo do
    balances[balIndex] := value;

