{ ---------------------------------------------------------------------------- }
{ -- setMonthBalance - Saves a single balance for the required account code    }
{                      and month                                               }
{ ---------------------------------------------------------------------------- }
function THCAccountingEngine.setMonthBalance(year: TBalanceYear; accountCode: PChar; month: integer; value: currency): boolean;
var
  tempBalancesMa: TMonthlyBalArray;
begin

  result := False;

  if getMonthlyBalances(year, accountCode, tempBalancesMa)
  then begin
    if monthBalRec.balancesMa[month] <> value
    then begin
      monthBalRec.balancesMa[month] := value;
      if fileAccess(monthBalFile, DB_SAVE, ByStdKey, PChar(FmonthBalKey), DB_NOLOCK) <> DB_TRUE then
        raise EHCAccountingEngine.Create('Failed to update monthly balances');
    end;
    result := True;
  end
  else if Value <> 0
  then begin
    // Make sure our array of balances is nice nd clean
    FillChar(tempBalancesMa, sizeof(TMonthlyBalArray), #0);
    tempBalancesMa[month] := value;
    result := setMonthlyBalances(year, accountCode, tempBalancesMa);
  end;

end;

