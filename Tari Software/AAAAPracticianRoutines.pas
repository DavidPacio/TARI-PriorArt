    exit;

  // Update the weekly balance record with the values passed
  strPCopy(weekBalRec.balKeyC, balanceKey);
  Move(balancesMa, weekBalRec.balancesMa, sizeof(TWeeklyBalArray));
  // jm 26.06.98 If the total of the balances about to be saved is zero then we delete the record
  // rather then have it stored.
  if totalBalanceArray(balancesMa) <> 0
  then begin
    if fileAccess(weekBalFile, DB_SAVE, ByStdKey, PChar(balanceKey), DB_NOLOCK) <> DB_TRUE then
      raise EHCAccountingEngine.Create('Failed to save weekly balances');
  end
  else begin
    if fileAccess(weekBalFile, DB_GET, ByStdKey, PChar(balanceKey), DB_LOCK) = DB_TRUE then
      if fileAccess(weekBalFile, DB_DELETE, ByStdKey, PChar(balanceKey), DB_NOLOCK) <> DB_TRUE then
        raise EHCAccountingEngine.Create('Failed to delete weekly balance record');
  end;
  
  result := True;
end;

