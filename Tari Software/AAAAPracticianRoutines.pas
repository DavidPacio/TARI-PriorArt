{ ---------------------------------------------------------------------------- }
{ -- setWeeklyBalancesValue - Really the same as above but make code easier to }
{                             follow what type of balance array we are dealing }
{                             with.                                            }
{                             tww 1.12.98 added toperiod to correctly          }
{                             initialise the array                             }
{ ---------------------------------------------------------------------------- }
procedure setWeeklyBalancesValue(var balances: array of currency; value: currency; toperiod: integer);
var
  balIndex: integer;
begin
  // First element is the opening balance
  for balIndex := 1 to high(balances) do
    if balindex <= toperiod then
      balances[balIndex] := value
    else
      balances[balIndex] := 0;
end;

{ ---------------------------------------------------------------------------- }
{ -- annualiseDataArray - Annualises the passed balances using the values in   }
{                         daysInPeriod1 (Source balance number of day in period
{                         daysInperiod2 (Dest. balances number of days in period
{ ---------------------------------------------------------------------------- }
procedure annualiseDataArray(var balToAnnualise: currency; daysInPeriod1, daysInPeriod2: extended);
var
  dailyBalance: currency;
begin
  // tww 1.12.98 if the difference in the days is less than 5 in 365 then no change
  if (abs(daysinperiod1/daysinPeriod2) > 1.014) or (abs(daysinPeriod1/daysinPeriod2) < 0.986)
    then begin
    // First work out a daily balance using the source days in period (daysInPeriod1)
    dailyBalance := balToAnnualise/daysInPeriod1;

    // Now annualise the daily balance using the destination days in period (daysInPeriod2)
    balToAnnualise := dailyBalance * daysInPeriod2;
    roundCurrency(balToAnnualise, 2);
    end;
end;

