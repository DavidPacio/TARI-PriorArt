{ ---------------------------------------------------------------------------- }
{ -- getMonthlyBalances - Gets the monthly balance array for the passed account}
{                         code.                                                }
{ ---------------------------------------------------------------------------- }
function THCAccountingEngine.getMonthlyBalances(year: TBalanceYear; accountCode: PChar; var balancesMa: TMonthlyBalArray): boolean;
var
  balanceFound: boolean;
  balanceKey: string;
begin

  // If in consolidation mode then pass request onto appropriate routine
  if assigned(ConsolidatedDivEngine)
  then begin
    result := getConsolidatedMonthlyBalances(year, accountCode, balancesMa);
    exit;
  end;

  balanceFound := False;

  // Build the unique key
  balanceKey := buildMonthBalanceKey(year, accountCode);

  // If balance key blank then no record to be found
  if balanceKey = ''
  then begin
    result := False;
    exit;
  end;

  // See if the balance key is same as current record key, in which case no
  // point in getting it again
  if balanceKey = FmonthBalKey then
     balanceFound := True

  // otherwise get the monthly balance record
  else if fileAccess(monthBalFile, DB_GET, ByStdKey, PChar(balanceKey), DB_NOLOCK) = DB_TRUE
  then begin
    // Record found so update internal key
    FmonthBalKey := balanceKey;
    balanceFound := True;
  end;

  // If balance found the get the copy balance array into one passed
  if balanceFound then
    Move(monthBalRec.balancesMa, balancesMa, sizeof(TMonthlyBalArray));

  result := balanceFound;
end;

{ ---------------------------------------------------------------------------- }
{ -- getConsolidatedMonthlyBalances - Gets the monthly balance array total for }
{                                     all divisions in the consolidate list    }
{ ---------------------------------------------------------------------------- }
function THCAccountingEngine.getConsolidatedMonthlyBalances(year: TBalanceYear; accountCode: PChar; var balancesMa: TMonthlyBalArray): boolean;
var
  divIndex: integer;
  balanceFound: boolean;
  balanceKey: string;
  conDivRecPtr: TConDivionDetailsPtr;
  retVal: longint;
begin

  // Assume the worse
  balanceFound := False;

  // Zero out the balance array to
  fillChar(balancesMa, sizeof(TMonthlyBalArray), #0);

  // Go through each division in the list, get each balance and merge into the list
  for divIndex := 0 to ConsolidatedDivEngine.DivToConsolidateCount - 1
  do begin
    conDivRecPtr := TConDivionDetailsPtr(ConsolidatedDivEngine.DivToConsolidate[divIndex]);
    if FDataSection in [dsPlanning, dsMonitoring] then
      currentYearKey := conDivRecPtr.planCurrentYearKey
    else
      currentYearKey := conDivRecPtr.anlCurrentYearKey;

    // Build the unique balance key
    balanceKey := buildMonthBalanceKey(year, accountCode);

    // If balance key blank then no record to be found for this division
    if balanceKey = '' then
      continue;

    // otherwise get the monthly balance record
    retVal := fileAccess(monthBalFile, DB_GET, ByStdKey, PChar(balanceKey), DB_NOLOCK);
    if  retVal = DB_TRUE
    then begin
      // Record found so update internal key
      FmonthBalKey := balanceKey;
      balanceFound := True;
      // Add the division balances to the total
      addBalanceArray(balancesMa, monthBalRec.balancesMa)
    end;

  end;

  result := balanceFound;

end;

{ ---------------------------------------------------------------------------- }
{ -- setMonthlyBalances - Saves the monthly balance array for the passed       }
{                         account code                                         }
{ ---------------------------------------------------------------------------- }
function THCAccountingEngine.setMonthlyBalances(year: TBalanceYear; accountCode: PChar; var balancesMa: TMonthlyBalArray): boolean;
var
  balanceKey: string;
begin
  result := False;

  // If in consolidate division mode then calling set should not be allowed.
  if assigned(ConsolidatedDivEngine) then
    raise EHCAccountingEngine.Create('Not allowed to save balances when in consolidate division mode');

  // Build the unique key
  balanceKey := buildMonthBalanceKey(year, accountCode);

  // If balance key blank then no record to be found
  if balanceKey = '' then
    exit;

  // Update the monthly balance record with the values passed
  strPCopy(monthBalRec.balKeyC, balanceKey);
  Move(balancesMa, monthBalRec.balancesMa, sizeof(TMonthlyBalArray));

  if fileAccess(monthBalFile, DB_SAVE, ByStdKey, PChar(balanceKey), DB_NOLOCK) <> DB_TRUE then
    raise EHCAccountingEngine.Create('Failed to save monthly balances');

  result := True;
end;

